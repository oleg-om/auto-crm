{
    "presets": [
      ["@babel/preset-env", {
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
  
    "plugins": [
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-class-properties",
      ["@babel/plugin-proposal-decorators",{ "legacy": true }]
    ]
  }